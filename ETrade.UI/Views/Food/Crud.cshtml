                <label asp-for="SelectedFood.CreatedDate" class="control-label"></label>
                <input asp-for="SelectedFood.CreatedDate" class="form-control" />
                <span asp-validation-for="SelectedFood.CreatedDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="@Model.Text" class="@Model.Class" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

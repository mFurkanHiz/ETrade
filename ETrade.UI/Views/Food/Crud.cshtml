            <div class="form-group">
                <label asp-for="SelectedFood.PropertyId" class="control-label"></label>
                <input asp-for="SelectedFood.PropertyId" class="form-control" />
                <span asp-validation-for="SelectedFood.PropertyId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="SelectedFood.Description" class="control-label"></label>
                <input asp-for="SelectedFood.Description" class="form-control" />

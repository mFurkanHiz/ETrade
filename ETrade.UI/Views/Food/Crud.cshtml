                <input asp-for="SelectedFood.FoodName" class="form-control" />
                <span asp-validation-for="SelectedFood.FoodName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="SelectedFood.CategoryId" class="control-label"></label>
